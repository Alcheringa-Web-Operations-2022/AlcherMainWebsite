  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "npx prettier '{src}/**/*.{ts,tsx,js}' --write && npx eslint '{src}/**/*.{ts,tsx,js}' --quiet --fix",
    "eslint": "npx eslint '{src}/**/*.{ts,tsx,js}' --quiet",
    "eslint:fix": "npx eslint '{src}/**/*.{ts,tsx,js}' --quiet --fix",
    "prettier": "npx prettier '{src}/**/*.{ts,tsx,js}' --check",
    "prettier:fix": "npx prettier '{src}*/**/*.{ts,tsx,js}' --write",
    "storybook": "npx start-storybook -p 6006 -c .storybook",
    "storybook:build": "npx build-storybook",
    "prepare": "husky install"
  },
  "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "prettier --write"
        ]
    },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
