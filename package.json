{
  "name": "r3f-cra-starter",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.3.0",
    "gsap": "^3.9.1",
    "locomotive-scroll": "^4.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-lazy-load-image-component": "^1.5.1",
    "react-locomotive-scroll": "^0.2.0",
    "react-router-dom": "^6.2.1",
    "react-scripts": "4.0.3",
    "react-slick": "^0.28.1",
    "sass": "^1.42.1",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@types/node": "^15.12.2",
    "@types/node-sass": "^4.11.1",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^16.9.14",
    "@types/react-responsive": "^8.0.2",
    "@types/sass": "^1.16.0",
    "@types/sass-loader": "^8.0.1",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "react-test-renderer": "^17.0.2",
    "sass-loader": "10.1.1",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "lint": "npx prettier 'src/**/*.{ts,tsx}' --write && npx eslint 'src/**/*.{ts,tsx}' --quiet --fix",
    "eslint": "npx eslint '{src}/**/*.{ts,tsx,js}' --quiet",
    "eslint:fix": "npx eslint '{src}/**/*.{ts,tsx,js}' --quiet --fix",
    "prettier": "npx prettier '{src}/**/*.{ts,tsx,js}' --check",
    "prettier:fix": "npx prettier '{src}*/**/*.{ts,tsx,js}' --write",
    "storybook": "npx start-storybook -p 6006 -c .storybook",
    "storybook:build": "npx build-storybook",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
